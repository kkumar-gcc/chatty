<div
  class="modal fade"
  id="modalRequest"
  data-bs-backdrop="static"
  data-bs-keyboard="false"
  tabindex="-1"
  aria-labelledby="modalRequestLabel"
  aria-hidden="true"
>
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalRequestLabel">Requests</h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body">
        <div id="fb">
          <div id="fb-top">
            <p>
              <b>Friend Requests</b>
              <!-- <span>Find Friends &bull; Settings</span> -->
            </p>
          </div>
          <div class="fb-bottom">
            <img
              src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/195612/chat_avatar_01.jpg"
              height="100"
              width="100"
              alt="Image of woman"
            />
            <p id="info"><b>Natalie G.</b></p>
            <div id="button-block">
              <div id="confirm">Confirm</div>
              <div id="delete">Delete Request</div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
